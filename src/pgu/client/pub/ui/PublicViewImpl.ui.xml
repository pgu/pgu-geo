<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
            xmlns:g="urn:import:com.google.gwt.user.client.ui"
            xmlns:b="urn:import:com.github.gwtbootstrap.client.ui" >

    <ui:style>
        .profile_section {
            padding-top: 0px;
        }
        .public_profile_map_area {
            padding-top: 20px;
        }
        #pgu_geo_public_profile_map { 
            height: 600px; 
            position: relative; 
            background-color: #E5E3DF;
            overflow: hidden;
        }
        #pgu_geo_public_profile_map img { max-width:none; }
    </ui:style>            

    <g:HTMLPanel>
<!--         <g:HTMLPanel ui:field="submenu" addStyleNames="subnav subnav-fixed"> -->
<!--             <b:NavPills> -->
<!--                 <b:NavLink href="#public:profile">Profile</b:NavLink> -->
<!--                 <b:NavLink href="#public:contacts">Contacts</b:NavLink> -->
<!--             </b:NavPills> -->
<!--         </g:HTMLPanel> -->

        <b:Section ui:field="profileSection" addStyleNames="{style.profile_section}">
            <g:HTMLPanel addStyleNames="row-fluid">
                <div class="span12">
                    <g:HTMLPanel addStyleNames="page-header">
                        <h3>Profile</h3>
                    </g:HTMLPanel>
                </div>
            </g:HTMLPanel>
            <g:HTMLPanel addStyleNames="row-fluid {style.public_profile_map_area}">
                <div class="span12">
                    <div id="pgu_geo_public_profile_map_container" class="collapse in">
                        <div id="pgu_geo_public_profile_map">
                        </div>
                    </div>
                </div>
            </g:HTMLPanel>
        </b:Section>    
        
        <b:Section ui:field="contactsSection">
            <g:HTMLPanel addStyleNames="row-fluid">
                <div class="span12">
                    <g:HTMLPanel addStyleNames="page-header">
                        <h3>Contacts</h3>
                    </g:HTMLPanel>
                </div>
            </g:HTMLPanel>
            <g:HTMLPanel addStyleNames="row-fluid">
                <div class="span12">
                </div>
            </g:HTMLPanel>
        </b:Section>    
    
    </g:HTMLPanel>
</ui:UiBinder> 