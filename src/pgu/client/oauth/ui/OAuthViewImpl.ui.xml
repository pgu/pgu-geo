<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
            xmlns:g="urn:import:com.google.gwt.user.client.ui"
            xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
    <ui:style>
    </ui:style>
    <g:HTMLPanel>
        <g:Anchor ui:field="oauthLink">Authorization ></g:Anchor>
        <b:WellForm>
            <b:ControlLabel>Authorization code</b:ControlLabel>
            <b:TextBox ui:field="oauthCodeInput" placeholder="code..."/>
            <b:Button ui:field="submitBtn" type="PRIMARY">Submit</b:Button>
        </b:WellForm>
    </g:HTMLPanel>
</ui:UiBinder> 